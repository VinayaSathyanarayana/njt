    "react-dom": "^16.12.0",
    "styled-components": "^5.0.1",
    "styled-normalize": "^8.0.7",
    "unfetch": "^4.1.0",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@types/hosted-git-info": "^3.0.0",
    "@types/lru-cache": "^5.1.0",
    "@types/node": "^13.7.6",
    "@types/node-fetch": "^2.5.5",
    "@types/styled-components": "4.1.19",
    "ncp": "^2.0.0",
    "prettier": "^1.19.1",
    "prettier-plugin-packagejson": "^2.0.10",
    "typescript": "^3.8.2",
