  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "fix": "yarn fix:prettier; yarn fix:yarn-deduplicate",
    "fix:prettier": "prettier --write \"**/*\"",
    "fix:yarn-deduplicate": "yarn-deduplicate --strategy=fewer",
    "lint": "yarn lint:prettier && yarn lint:tsc && yarn fix:yarn-deduplicate",
    "lint:prettier": "prettier --check \"**/*\"",
    "lint:tsc": "tsc --project .",
    "lint:yarn-deduplicate": "yarn-deduplicate --fail --list --strategy=fewer",
    "start": "next start"
  },
  "dependencies": {
    "@zeit/fetch": "^5.2.0",
    "hosted-git-info": "^3.0.3",
    "lru-cache": "^5.1.1",
    "next": "^9.2.2",
